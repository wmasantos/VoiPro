        <shape android:shape="oval">
            <solid android:color="#6A1B9A"/>
            <stroke android:width="1dp" android:color="#FFFFFF"/>
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <solid android:color="#CE93D8"/>
            <stroke android:width="1dp" android:color="#FFFFFF"/>
        </shape>
    </item>
</selector>